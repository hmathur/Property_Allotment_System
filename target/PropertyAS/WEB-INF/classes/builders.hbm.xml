<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="bean.Builders" table="builders">
        <id name="bId" type="java.lang.Integer">
            <column name="BUILDER_ID" />
            <generator class="assigned" />
        </id>
        <property name="bName" type="java.lang.String">
        	<column name="BUILDER_NAME" />
        </property>
        
        <property name="lId" type="java.lang.Integer">
            <column name="LOCATION_ID" />
        </property> 
        
         <property name="cityCode" type="java.lang.Integer">
            <column name="CITY_CODE" />
        </property>       
    </class>
</hibernate-mapping>