<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.xavient.pas.dao.bean.Apartment" table="apartment">
        <id name="apartmentId" type="java.lang.Integer">
            <column name="APARTMENT_ID" />
            <generator class="assigned" />
        </id>
        <property name="apartmentName" type="java.lang.String">
        	<column name="APARTMENT_NAME" />
        </property>
        
       <!--   <property name="apartmentName" type="java.lang.String">
        	<column name="APARTMENT_NAME" />
        </property> -->
        
        <property name="schemeId" type="java.lang.Integer">
            <column name="SCHEME_ID" />
        </property> 
        
         <property name="builderId" type="java.lang.Integer">
            <column name="BUILDER_ID" />
        </property>     
        
         <property name="locationId" type="java.lang.Integer">
            <column name="LOCATION_ID" />
        </property>   
        
        <property name="cityCode" type="java.lang.Integer">
            <column name="CITY_CODE" />
        </property>   
        
         <property name="areaCode" type="java.lang.Integer">
        	<column name="AREA_CODE" />
        </property>
        
        <property name="distFromNearestAirport" type="java.lang.Integer">
            <column name="DIST_FROM_NEAREST_AIRPORT" />
        </property> 
        
         <property name="distFromNearestRailway" type="java.lang.Integer">
            <column name="DIST_FROM_NEAREST_RAILWAY" />
        </property>     
        
         <property name="distFromCenterOfCity" type="java.lang.Integer">
            <column name="DIST_FROM_CENTER_OF_CITY" />
        </property>   
        
        <property name="distFromNearestHospital" type="java.lang.Integer">
            <column name="DIST_FROM_NEAREST_HOSPITAL" />
        </property>   
        
       <property name="cost" type="java.lang.Integer">
            <column name="COST" />
        </property>     
        
         <property name="availabilty" type="java.lang.Boolean">
            <column name="AVAILABILITY" />
        </property>   
        
        <property name="floor" type="java.lang.Integer">
            <column name="FLOOR" />
        </property>   
    </class>
</hibernate-mapping>